\@xx{}%
 \@x{\@s{57.29} \.{\ELSE} \.{\LET} foidh\@s{2.59} \.{\defeq} {\CHOOSE} oid
 \.{\in} ctxDiffh \.{:}}%
\@y{%
  the first $oid$ in $ctxDiffh$ 
}%
\@xx{}%
 \@x{\@s{160.92} \A\, id \.{\in} ctxDiffh \.{\,\backslash\,} \{ oid \} \.{:}
 tb ( oid ,\, id ,\, serial [ r ] )}%
 \@x{\@s{109.01} fcoph\@s{1.57} \.{\defeq} {\CHOOSE} fcop \.{\in} copss [ r ]
 \.{:}}%
\@x{\@s{160.92} fcop . oid \.{=} foidh \.{\land} fcop . ctx \.{=} coph . ctx}%
\@y{%
  \cc
}%
\@xx{}%
\@x{\@s{109.01} xcoph \.{\defeq} COT ( coph ,\, fcoph )}%
\@x{\@s{100.91} xfcoph\@s{5.03} \.{\defeq} COT ( fcoph ,\, coph )}%
 \@x{\@s{88.61} \.{\IN}\@s{4.1} xFormHelper ( xcoph ,\, ctxDiffh
 \.{\,\backslash\,} \{ fcoph . oid \} ,\,}%
\@x{\@s{204.92} copssh \.{\cup} \{ xcoph ,\, xfcoph \} )}%
 \@x{\@s{16.4} \.{\IN}\@s{4.09} xFormHelper ( cop ,\, ctxDiff ,\, copss [ r ]
 \.{\cup} \{ cop \} )}%
